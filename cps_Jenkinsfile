node() {
    
    def items = ["cloud", "jenkins", "java", "nhn"]
    func(items)
    
    def map = [
    'FF0000' : 'Red',
    '00FF00' : 'Lime',
    '0000FF' : 'Blue',
    'FFFF00' : 'Yellow'
]

    func2(map)
}

def func2(map) {
    map.each { println "Hex Code: $it.key = Color Name: $it.value" }
}

def func(items) {
    items.each {
        echo it        
    }
}